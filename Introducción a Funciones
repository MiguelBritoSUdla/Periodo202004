#include <stdio.h>
#include <math.h>
int maximo(int,int);

int main()
{
    
   int x,y;
   printf("Ingrese el valor de x: ");
   scanf("%i",&x);
   printf("Ingrese el valor de y: ");
   scanf("%i",&y);

   maximo(x,y);
   printf("La suma es de %i", maximo(x,y));
   return 0;
    
}

int maximo (int a, int b) 
{
    int suma;
    suma=a+b;
    return suma;
}
