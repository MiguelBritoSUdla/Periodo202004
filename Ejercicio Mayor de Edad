#include <stdio.h>
void main()
{
    
    int edad;
    char nombre[10];
    printf("Ingrese el nombre ");
    gets(nombre);
    printf("Ingrese su edad");
    scanf("%d",&edad);
    if (edad>=18)
    {
        printf("%s es mayor de edad",nombre);
    }
    else
    {
        printf("%s no es mayor de edad", nombre);
    }
    
}
